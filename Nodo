public abstract class Nodo<E> implements Comparable<E>{

    E Valor;
    Nodo Siguiente;           
    Nodo Anterior;
    
    public  void setSiguiente(Nodo nodo){
        Siguiente = nodo;
        }
    public  Nodo getSiguiente(){
        return Siguiente;
        }
    public E getValor(){
        return Valor;
        }
    public  void setValor(E Valor){
        this.Valor = Valor;
        }
    @Override
    public String toString(){
        return ("Tostring");
    }
        }
    
