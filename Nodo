public abstract class Nodo<E implements Comparable>{

    <E> Valor;
    Nodo Siguiente;           
    
    public abstract void setSiguiente(Nodo<E> nodo){
        Siguiente = nodo;
        }
    public abstract Nodo<E> getSiguiente(){
        return Siguiente;
        }
    public abstract <E> getValor(){
        return Valor;
        }
    public abstract void setValor(Valor){
        this.Valor = Valor;
        }
    public abstract String toString(){
        }
    
    public int compareTo(Nodo<E> other) {
		return Valor.compareTo(other);
}
